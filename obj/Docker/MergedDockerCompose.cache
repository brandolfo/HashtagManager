services:
  hashtagmanager:
    build:
      context: D:\Programming\Projects\Brandom\HashtagManager
      dockerfile: Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: https://+:443;http://+:80
    image: hashtagmanager
    ports:
    - target: 80
    - target: 443
    volumes:
    - C:\Users\Corvo\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\Corvo\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
version: '3.4'